}
.progress-radial {
	BORDER-BOTTOM: #fff 2px solid; POSITION: relative; BORDER-LEFT: #fff 2px solid; BACKGROUND-COLOR: #f5f5f5; WIDTH: 70px; DISPLAY: inline-block; HEIGHT: 70px; BORDER-TOP: #fff 2px solid; BORDER-RIGHT: #fff 2px solid; border-radius: 50%
}
.progress-radial:after {
	Z-INDEX: 999; POSITION: absolute; MARGIN-TOP: -17.5px; WIDTH: 35px; HEIGHT: 35px; MARGIN-LEFT: -17.5px; CONTENT: ''; TOP: 50%; LEFT: 50%; border-radius: 50%
