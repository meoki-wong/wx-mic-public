}
.m-nvestment I {
	DISPLAY: inline-block; FLOAT: left
}
.m-nvestment I.s-01 {
	MARGIN: 8px 10px 0px 0px; WIDTH: 40px; BACKGROUND: url(../images/index/pio-icon.png) no-repeat; HEIGHT: 30px; background-size: 40px auto
}
.m-nvestment I.s-02 {
	MARGIN: 3px 10px 0px 20px; WIDTH: 39px; BACKGROUND: url(../images/index/pic-icon.png) no-repeat; HEIGHT: 41px; background-size: 40px auto
}
.m-nvestment DL .z-poi {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 50px; WIDTH: 76%; PADDING-RIGHT: 0px; PADDING-TOP: 0px
}
.m-nvestment DL DT {
	COLOR: #646464; FONT-SIZE: 12px
}
.m-nvestment DL DD {
	COLOR: #da0000; FONT-SIZE: 16px
