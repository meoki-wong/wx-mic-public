BODY {
	LINE-HEIGHT: 22px; BACKGROUND-COLOR: #f4f4f1; COLOR: #555
}
A {
	COLOR: #555
}
.font-yen {
	FONT-FAMILY: "Microsoft YaHei", STXihei, MingLiu
}
.clearfix:after {
	DISPLAY: block; HEIGHT: 0px; VISIBILITY: hidden; CLEAR: both; CONTENT: "."
}
HTML .clearfix {
	HEIGHT: 1%
}
.clearfix {
	DISPLAY: block
}
INPUT[type='text'] {
	OUTLINE-STYLE: none; OUTLINE-COLOR: invert; OUTLINE-WIDTH: 0px; -webkit-appearance: none; -moz-appearance: none; -webkit-tap-highlight-color: transparent; -webkit-transition: background-color
}
INPUT[type='number'] {
	OUTLINE-STYLE: none; OUTLINE-COLOR: invert; OUTLINE-WIDTH: 0px; -webkit-appearance: none; -moz-appearance: none; -webkit-tap-highlight-color: transparent; -webkit-transition: background-color
}
INPUT[type='password'] {
	OUTLINE-STYLE: none; OUTLINE-COLOR: invert; OUTLINE-WIDTH: 0px; -webkit-appearance: none; -moz-appearance: none; -webkit-tap-highlight-color: transparent; -webkit-transition: background-color
}
INPUT[type='button'] {
	OUTLINE-STYLE: none; OUTLINE-COLOR: invert; OUTLINE-WIDTH: 0px; -webkit-appearance: none; -moz-appearance: none; -webkit-tap-highlight-color: transparent; -webkit-transition: background-color
}
INPUT[type='submit'] {
	OUTLINE-STYLE: none; OUTLINE-COLOR: invert; OUTLINE-WIDTH: 0px; -webkit-appearance: none; -moz-appearance: none; -webkit-tap-highlight-color: transparent; -webkit-transition: background-color
}
TEXTAREA {
	OUTLINE-STYLE: none; OUTLINE-COLOR: invert; OUTLINE-WIDTH: 0px; -webkit-appearance: none; -moz-appearance: none; -webkit-tap-highlight-color: transparent; -webkit-transition: background-color
}
BUTTON {
	OUTLINE-STYLE: none; OUTLINE-COLOR: invert; OUTLINE-WIDTH: 0px; -webkit-appearance: none; -moz-appearance: none; -webkit-tap-highlight-color: transparent; -webkit-transition: background-color
}
.header {
	WIDTH: 100%; BACKGROUND: #005bac; HEIGHT: 60px
}
.header H1 {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 10px; PADDING-RIGHT: 0px; FLOAT: left; HEIGHT: 35px; VERTICAL-ALIGN: middle; PADDING-TOP: 0px
}
.header H1 A {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; DISPLAY: inline-block; PADDING-TOP: 12px
}
.header H1 A IMG {
	WIDTH: 159px; HEIGHT: 36px
}
.header .m-login {
	PADDING-BOTTOM: 0px; LINE-HEIGHT: 58px; PADDING-LEFT: 0px; PADDING-RIGHT: 10px; FLOAT: right; PADDING-TOP: 0px
}
.header .m-login I {
	COLOR: #fff; VERTICAL-ALIGN: 2px
}
.header .m-login A {
	COLOR: #fff; FONT-SIZE: 16px
}
footer {
	TEXT-ALIGN: center; PADDING-BOTTOM: 0px; MARGIN: 20px 0px 0px; PADDING-LEFT: 0px; WIDTH: 100%; PADDING-RIGHT: 0px; BORDER-TOP: #ccc 1px solid; PADDING-TOP: 10px
}
footer P {
	COLOR: #b5b5b5; FONT-SIZE: 14px
}
footer P EM {
	COLOR: #015caf
}
footer P A {
	COLOR: #015caf
}
.s-muse {
	BACKGROUND: #b17c44
}
.content-body {
	MIN-HEIGHT: 450px
}
.navigation-bar {
	BORDER-BOTTOM: #dfdfdf 1px solid; POSITION: relative; LINE-HEIGHT: 50px; BACKGROUND: #fff; COLOR: #010000
}
.navigation-bar A.z-01 {
	POSITION: absolute; MARGIN-TOP: -11px; WIDTH: 13px; BACKGROUND: url(../images/index/fanhui.png) no-repeat; HEIGHT: 22px; TOP: 50%; LEFT: 10px; background-size: 13px 22px
}
.navigation-bar A.z-02 {
	POSITION: absolute; MARGIN-TOP: -14px; WIDTH: 25px; BACKGROUND: url(../images/index/poi.png) no-repeat; HEIGHT: 27px; TOP: 50%; RIGHT: 10px; background-size: 25px 27px
}
.navigation-bar SPAN {
	TEXT-ALIGN: center; WIDTH: 100%; DISPLAY: inline-block; FONT-SIZE: 20px
}
.m-nav {
	BORDER-BOTTOM: #dfdfdf 1px solid; LINE-HEIGHT: 50px; WIDTH: 100%; BACKGROUND: #fff; HEIGHT: 50px
}
.m-nav LI {
	TEXT-ALIGN: center; WIDTH: 24%; FLOAT: left
}
.m-nav LI A {
	DISPLAY: block; COLOR: #5a5a5a; FONT-SIZE: 16px
}
.m-nav LI A.z-active {
	BORDER-BOTTOM: #005bac 2px solid; HEIGHT: 48px; COLOR: #005bac
}
.z-subnav {
	Z-INDEX: 100; POSITION: fixed; WIDTH: 100%; TOP: 0px
}
.load-more {
	TEXT-ALIGN: center; LINE-HEIGHT: 50px; WIDTH: 100%; DISPLAY: inline-block; COLOR: #b4b4b4; FONT-SIZE: 15px
}
.load-more IMG {
	WIDTH: 13px; HEIGHT: 15px; VERTICAL-ALIGN: middle
}
.bg-layer {
	Z-INDEX: 999; POSITION: fixed; WIDTH: 100%; BACKGROUND: url(../images/icon/bg-back_03.png); HEIGHT: 100%; TOP: 0px
}
.name-layer {
	POSITION: absolute; TEXT-ALIGN: center; PADDING-BOTTOM: 10px; PADDING-LEFT: 0px; WIDTH: 240px; PADDING-RIGHT: 0px; BACKGROUND: #fff; TOP: 50%; PADDING-TOP: 0px; LEFT: 50%; -webkit-transform: translate(-50%, -50%); transform: translate(-50%, -50%); box-shadow: 0 0 10px 0px rgba(0, 0, 0, 0.5)
}
.name-layer H4 {
	PADDING-BOTTOM: 10px; LINE-HEIGHT: 25px; PADDING-LEFT: 20px; PADDING-RIGHT: 20px; COLOR: #666; FONT-SIZE: 16px; PADDING-TOP: 10px
}
.name-layer P A {
	LINE-HEIGHT: 35px; WIDTH: 85px; DISPLAY: inline-block; BACKGROUND: #2472b8; HEIGHT: 35px; COLOR: #fff; FONT-SIZE: 16px
}
.name-layer P A.the-certification {
	MARGIN: 0px 10px 0px 0px; BACKGROUND: #ff9900
}
.m-turnplate {
	POSITION: absolute; MARGIN-TOP: -35px; WIDTH: 70px; HEIGHT: 70px; TOP: 50%; RIGHT: 15px
}
.m-turnplate01 {
	POSITION: absolute; MARGIN-TOP: -40px; WIDTH: 70px; HEIGHT: 70px; TOP: 50%; RIGHT: 32px
}
.m-turnplate02 {
	POSITION: absolute; MARGIN-TOP: -60px; WIDTH: 70px; HEIGHT: 70px; TOP: 50%; RIGHT: 10px
}
.has-reached IMG {
