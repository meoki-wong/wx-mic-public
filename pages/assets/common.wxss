INPUT.error {
	BORDER-BOTTOM-COLOR: #ebccd1; BACKGROUND-COLOR: #f2dede; BORDER-TOP-COLOR: #ebccd1; COLOR: #a94442; BORDER-RIGHT-COLOR: #ebccd1; BORDER-LEFT-COLOR: #ebccd1
}
FORM .submit {
	POSITION: relative
}
FORM .loading.submit {
	BACKGROUND: #cdcdcd; box-shadow: none
}
FORM .submit I {
	WIDTH: 1.3em; DISPLAY: none; BACKGROUND: url(../images/spinner2.gif) no-repeat; HEIGHT: 1.3em; VERTICAL-ALIGN: text-bottom; MARGIN-RIGHT: 0.5em; background-size: contain
}
FORM .loading.submit I {
	DISPLAY: inline-block
}
FORM .shake {
	-webkit-animation-name: "shake"; -webkit-animation-duration: 0.4s; -webkit-transform-origin: 50% 50%; -webkit-animation-iteration-count: 1; -webkit-animation-timing-function: linear
}
FORM .blink {
	-webkit-animation-name: "blink"; -webkit-animation-duration: 1s; -webkit-transform-origin: 50% 50%; -webkit-animation-iteration-count: 1; -webkit-animation-timing-function: linear
}
FORM .swing {
	-webkit-animation-name: "swing"; -webkit-animation-duration: 0.4s; -webkit-transform-origin: 50% 50%; -webkit-animation-iteration-count: 1; -webkit-animation-timing-function: linear
}
.banner {
	OVERFLOW: hidden; -webkit-transition: height .4s
}
.alert {
	TEXT-ALIGN: center; PADDING-BOTTOM: 0.75em; PADDING-LEFT: 12px; PADDING-RIGHT: 12px; COLOR: #fff; FONT-SIZE: 18px; FONT-WEIGHT: 700; PADDING-TOP: 0.75em
}
.alert-danger.alert {
	BACKGROUND: #e74c3c
}
.alert-success.alert {
	BACKGROUND: #2bd672
}
.modal-layer {
	Z-INDEX: 9999; POSITION: fixed; WIDTH: 100%; HEIGHT: 100%; TOP: 0px
}
.modal-layer .mask {
	Z-INDEX: 1; POSITION: absolute; WIDTH: 100%; HEIGHT: 100%; TOP: 0px; LEFT: 0px
}
.modal-layer .content {
	Z-INDEX: 2; POSITION: relative; MARGIN: -150px auto 0px; TOP: 50%; box-shadow: 0 2px 10px 1px
}
.spinner {
	MARGIN: 1.5em auto; WIDTH: 1.5em; DISPLAY: none; BACKGROUND: url(../images/spinner.gif) no-repeat; HEIGHT: 1.5em; background-size: contain
}
.loading .spinner {
	DISPLAY: block
}
.loading .load-more {
	DISPLAY: none
}
.btn {
	TEXT-ALIGN: center; LINE-HEIGHT: 65px; MARGIN: 15px auto 0px; WIDTH: 100%; DISPLAY: block; HEIGHT: 65px; FONT-SIZE: 24px; border-radius: .25em
}
.btn-default {
	BACKGROUND: #ddd; COLOR: #666; box-shadow: inset 0 -3px 0 rgba(0, 0, 0, .135)
}
.btn-blue {
	BACKGROUND: #449fdb; COLOR: #fff; box-shadow: inset 0 -3px 0 rgba(0, 0, 0, .25)
}
.btn-orange {
	BACKGROUND: #ffbb4d; COLOR: #fff; box-shadow: inset 0 -2px rgba(0, 0, 0, .25)
}
.btn-danger {
	BACKGROUND: #e74c3c; COLOR: #fff; box-shadow: inset 0 -3px rgba(0, 0, 0, .15)
}
.user-center {
	MARGIN-TOP: 18px; WIDTH: 24px; DISPLAY: block; BACKGROUND: url(../images/user-icon.png) no-repeat; HEIGHT: 24px; background-size: contain
}
